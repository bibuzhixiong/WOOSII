        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rl_shenqing_tuizu"
                >
                <ImageView
                    android:id="@+id/iv_shenqing_tuizu"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/shenhetuizu"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    />

                <View
                    android:id="@+id/line1"
                    android:background="@color/mianColor"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/topLineThick"
                    android:layout_toRightOf="@+id/iv_shenqing_tuizu"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    />
            </RelativeLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_below="@+id/rl_shenqing_tuizu"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerHorizontal="true"
                >

                <TextView
                    android:id="@+id/tv_shenqing_tuizu"
                    android:layout_width="70dp"
                    android:layout_height="20dp"
                    android:text="申请退租"
                    android:gravity="center"
                    android:textColor="@color/mianColor"
                    />

            </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rl_shenqing_shenhe"

                >
                <View
                    android:id="@+id/line2"
                    android:background="@color/mianColor"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/topLineThick"
                    android:layout_toLeftOf="@+id/iv_tuizu_shenhe"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    />
                <ImageView
                    android:id="@+id/iv_tuizu_shenhe"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/shenhetuizu"
                    android:layout_centerHorizontal="true"
                    android:padding="3dp"
                    />

                <View
                    android:id="@+id/line3"
                    android:background="@color/line_gray"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/topLineThick"
                    android:layout_toRightOf="@+id/iv_tuizu_shenhe"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    />
            </RelativeLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_below="@+id/rl_shenqing_shenhe"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerHorizontal="true"
                >

                <TextView
                    android:id="@+id/tv_tuizu_shenhe"
                    android:layout_width="70dp"
                    android:layout_height="20dp"
                    android:text="退租审核"
                    android:gravity="center"
                    android:textColor="@color/mianColor"
                    />
                <TextView
                    android:visibility="invisible"
                    android:id="@+id/tv_tuizu_shenhe_time_year_mouth_day"
                    android:layout_width="70dp"
                    android:layout_height="20dp"
                    android:text="2017.04.06"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    />
                <TextView
                    android:visibility="invisible"
                    android:id="@+id/tv_tuizu_shenhe_time_second"
                    android:layout_width="70dp"
                    android:layout_height="20dp"
                    android:text="09.32"
                    android:gravity="center"
                    />

            </LinearLayout>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="0dp"
