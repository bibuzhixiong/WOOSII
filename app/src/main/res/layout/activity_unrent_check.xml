                    android:id="@+id/tv_tuizu_success_time_second"
                    android:layout_width="70dp"
                    android:layout_height="20dp"
                    android:text="09.32"
                    android:gravity="center"
                    />

            </LinearLayout>

        </RelativeLayout>

