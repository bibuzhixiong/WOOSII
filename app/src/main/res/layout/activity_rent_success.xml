<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <include layout="@layout/activity_top_hold"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rent_success"
        android:layout_gravity="center_horizontal"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="租用成功"
        android:textSize="@dimen/textBigBiggerSize"
        android:textColor="@color/mainTextColor"
        android:gravity="center_horizontal"
        />
    <TextView
        android:id="@+id/tv_back_main"
        android:layout_marginTop="50dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="返回首页"
        android:textSize="@dimen/textPrimarySize"
        android:textColor="@color/white"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:background="@drawable/selector_login_or_register_pressed"
        android:gravity="center_horizontal"
        android:padding="8dp"

        />


</LinearLayout>